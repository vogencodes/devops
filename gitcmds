git status                          #displays location of files in developer environment
git init                            #initializes a local repo
git add                             #moves file from workingArea to stagingArea
git log                             #displays all commits
git commit -m                       #moves file from staging area to local repo
git show "commit id"                #displays changes made in a file
git remote -v                       #displays remote repository
git remote add aliasName repoURL    #to add remote repository
git push aliasName branchName       #pushes file from local repository to remote repository
